<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Huy Ngoan Store</title>
    <link rel="shortcut icon" href="~/Scripts/images/favicon.ico" type="image/x-icon">

    <!-- Bootstrap CSS -->
    @*<link rel="stylesheet" href="~/Scripts/css/assets/bootstrap.min.css">*@
    <link href="~/Scripts/css/assets/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Scripts/css/assets/font-awesome.min.css">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:600,700%7COpen+Sans:400,600" rel="stylesheet">
    <!-- Popup -->
    <link href="~/Scripts/css/assets/magnific-popup.css" rel="stylesheet">
    <!-- Main Menu-->
    <link rel="stylesheet" href="~/Scripts/css/assets/meanmenu.css">
    <!-- Custom CSS -->
    <link href="~/Assets/css/login.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Scripts/css/style.css">
    <link rel="stylesheet" href="~/Scripts/css/responsive.css">
    <!--toastr-->
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <style>
        .single-courses_v header .header-body {
            background: white;
            min-height: 90px;
            background-size: cover;
            position: relative;
        }
    </style>
</head>

<body ng-app="app" class="single-courses_v single-courses_v1" ng-controller="LoginController">
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>
    <header id="header">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-xs-12 header-top-left">
                        <ul class="list-unstyled">
                            <li><i class="fa fa-phone top-icon"></i> +123 456 789</li>
                            <li><i class="fa fa-envelope top-icon"></i> hello@eduread.com</li>
                        </ul>
                    </div>
                    <div class="col-sm-6 col-xs-12 header-top-right">
                        <ul class="list-unstyled" ng-hide="hi">
                            <li><a ng-href="/Login/Register"><i class="fa fa-user-plus top-icon"></i>Đăng ký</a></li>
                            <li><a ng-href="/Login/Index"><i class="fa fa-lock top-icon"></i>Đăng nhập</a></li>

                        </ul>

                        <ul class="list-unstyled" ng-show="hi">
                            <li ng-init="Get_UserLogged()"><a ng-href="/Login/Info">Hello, {{User.Name}}</a></li>
                            <li><a ng-href="#" ng-click="LogOut()">Đăng Xuất</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div><!-- Ends: .header-top -->

        <div class="header-body">
            <nav class="navbar edu-navbar">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a ng-href="/Home/Index" class="navbar-brand  data-scroll">@*<img src="~/Scripts/images/Logo/logo-wide.png" alt="">*@<span>Huy Ngoan Store</span></a>
                    </div>

                    @{Html.RenderPartial("Menu"); }
                </div>
                <!-- /.container -->
            </nav>
        </div>


    </header>
    <div class="main-content">
    <!--  End header section-->
    <section ng-controller="LoginController">
        <div class="container">
            <section class="blog_1 col-sm-3" style="border-right: 1px solid #bdc3c7;">
                <div class="blog-area">
                    <div class="blog_side-right">
                        <div class="categories-item">
                            <img style="border-radius:30px; width: 50px; margin: 0 0 0 2.5em;" ng-src="~/Templatess/images/Logo/user.png" alt="user" />
                            <h5 style="padding-bottom: 1em;">{{User.Name}}</h5>
                            <ul class="list-unstyled">
                                <li><a ng-href="/Login/Info"><i class="fa fa-angle-right"></i>Tài khoản của tôi</a></li>
                                <li><a ng-href="/Cart/History"><i class="fa fa-angle-right"></i>Đơn mua</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
            @RenderBody()
        </div>
    </section>
        </div>

    <!-- Footer Area section -->
    <footer>
        <div class="container">
            <div class="row">
                <div class=" col-sm-12 footer-content-box">
                    <div class="col-sm-3">
                        <h3>@*<span><i class="fa fa-graduation-cap footer-h-icon"></i></span>*@ Huy Ngoan Store</h3>
                        <p>Thời trang cho mọi nhà.</p>
                        <ul class="list-unstyled">
                            <li><span><i class="fa fa-phone footer-icon"></i></span>098-999-9999</li>
                            <li><span><i class="fa fa-envelope footer-icon"></i></span>HuyNgoan@gmail.com</li>
                            <li><span><i class="fa fa-map-marker footer-icon"></i></span>Ân Thi, Hưng Yên, Việt Nam</li>
                        </ul>
                    </div>

                    <div class="col-sm-3">
                        <h3>Khám phá</h3>
                        <ul class="list-unstyled">
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>Về chúng tôi</a></li>
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>Câu hỏi thường gặp</a></li>
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>tin tức</a></li>
                        </ul>
                    </div>

                    <div class="col-sm-3">
                        <h3>Chính sách</h3>
                        <ul class="list-unstyled">
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>Chính sách vận chuyển</a></li>
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>Chính sách thanh toán</a></li>
                            <li><a href="#"><span><i class="fa fa-long-arrow-right footer-icon"></i></span>Hướng dẫn chọn size</a></li>
                        </ul>
                    </div>

                    <div class="col-sm-3">
                        <h3>Đăng ký ngay</h3>
                        <p>Đăng ký ngay hôm nay để nhận ngàn ưu đãi.</p>

                        <form action="#">
                            <div class="form-group">
                                <input placeholder="Your Email" type="email" required="">
                                <div class="submit-btn">
                                    <button type="submit" class="text-center">Subscribe</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        <div class="footer-bottom">
            <div class="container">
                <div class="footer-bottom-inner">
                    <div class="row">
                        <div class="col-md-6 col-sm-12 footer-no-padding">
                            <p>&copy; Copyright 2020 Annie | All rights reserved</p>
                        </div>
                        <div class="col-md-6 col-sm-12 footer-no-padding">
                            <ul class="list-unstyled footer-menu text-right">
                                <li>Follow us:</li>
                                <li><a href="#"><i class="fa fa-facebook facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-instagram instagram"></i></a></li>
                                <li><a href="#"><i class="fa fa-google-plus google"></i></a></li>
                                <li><a href="#"><i class="fa fa-skype skype"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- ./ End footer-bottom -->
    </footer>
    <!-- ./ End Footer Area-->
    <!-- ============================
    JavaScript Files
    ============================= -->
    <!-- jQuery -->
    @*<script src="~/Scripts/jquery-3.5.1.min.js"></script>*@
    <script src="~/Scripts/js/vendor/jquery-1.12.4.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/Scripts/js/assets/bootstrap.min.js"></script>
    <!-- owl carousel -->
    <script src="~/Scripts/js/assets/owl.carousel.min.js"></script>
    <!-- Revolution Slider -->
    <script src="~/Scripts/js/assets/revolution/jquery.themepunch.revolution.min.js"></script>
    <script src="~/Scripts/js/assets/revolution/jquery.themepunch.tools.min.js"></script>
    <!-- Popup -->
    <script src="~/Scripts/js/assets/jquery.magnific-popup.min.js"></script>
    <!-- Sticky JS -->
    <script src="~/Scripts/js/assets/jquery.sticky.js"></script>
    <!-- Counter Up -->
    <script src="~/Scripts/js/assets/jquery.counterup.min.js"></script>
    <script src="~/Scripts/js/assets/waypoints.min.js"></script>
    <!-- Slick Slider-->
    <script src="~/Scripts/js/assets/slick.min.js"></script>
    <!-- Main Menu -->
    <script src="~/Scripts/js/assets/jquery.meanmenu.min.js"></script>
    <!-- Custom JS -->
    <script src="~/Scripts/js/custom.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Angularjs/app.js"></script>
</body>
</html>
